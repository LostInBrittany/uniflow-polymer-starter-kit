{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-var": "off",
    "new-cap": [
      "error",
      {
        "capIsNewExceptions": [
          "Polymer"
        ]
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "globals": {
    "Polymer": true,
    "UniFlow": true,
    "myApp": true
  }
}